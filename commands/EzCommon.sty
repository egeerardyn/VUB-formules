                                        \foutenentry{Afleesfout}{#1}
                                        \foutenentry{Instelfout}{#1}
                                        \foutenentry{Instrumentfout}{#1}
                                        \foutenentry{Nulpuntsfout}{#1}
                                          \hline
                                        \foutenentry{\textbf{Totale absolute fout}}{#1}
                                          \hline
                                        \foutenentry{\textbf{Totale relatieve fout}}{}
                                      \end{tabular}
                                     \normtables
                                    }
